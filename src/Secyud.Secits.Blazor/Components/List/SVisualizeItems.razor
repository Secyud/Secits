@namespace Secyud.Secits.Blazor.Components
@inherits ScSettingBase<ISList<TItem>>
@typeparam TItem

@code
{
    public RenderFragment GenerateItemsContent() => __builder =>
    {
        if (Master is null) return;

        <Virtualize @ref="_virtualize" ItemsProvider="RefreshRowsAsync" Context="item">
            @Master.GenerateRow(item)
        </Virtualize>
    };
}
