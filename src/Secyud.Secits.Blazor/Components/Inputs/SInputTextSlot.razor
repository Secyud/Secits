@namespace Secyud.Secits.Blazor.Components
@inherits ScSettingBase<SInput<TValue>>
@typeparam TValue

@code
{
    public RenderFragment RenderSlot() =>
        @<input value="@InputString"
                readonly="@(Master.Readonly ? "readonly" : null)"
                disabled="@(Master.Disabled ? "disabled" : null)"
                @oninput="OnInputAsync" @onchange="@OnChangeAsync"
                @attributes="Master.Attributes"/>;
}
